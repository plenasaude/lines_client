{
  "name": "edoc",
  "version": "2.0.2",
  "description": "eDoc lines clients",
  "main": "main.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "start": "electron .",
    "build": "build --win --mac --x64 --ia32",
    "build:win": "build --win --x64 --ia32",
    "build:mac": "build --win --mac",
    "dist:win": "electron-builder --windows --x64 --ia32"
  },
  "author": "eDoc Saude <contato@edocsaude.com>",
  "build": {
    "appId": "com.github.edocsaude.edoc",
    "productName": "eDoc",
    "compression": "store",
    "publish": [
      {
        "provider": "github",
        "owner": "edocsaude",
        "repo": "lines_clients_test"
      }
    ],
    "nsis": {
      "perMachine": true
    }
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.13.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "bootstrap": "^3.3.7",
    "electron-log": "^2.2.7",
    "electron-updater": "^2.18.2",
    "moment": "^2.18.1",
    "ramda": "^0.24.1",
    "sweetalert2": "^6.6.6"
  }
}
